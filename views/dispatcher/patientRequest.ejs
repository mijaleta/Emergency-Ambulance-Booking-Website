    <script>



// JavaScript code


function deleteBookingRequest(bookingRequestId) {
    if (confirm('Are you sure you want to delete this booking request?')) {
        fetch('/booking-requests/' + bookingRequestId, {
            method: 'DELETE'
        })
        .then(response => {
            if (response.ok) {
                // Refresh the page or update the booking request list as needed
                location.reload(); // Refresh the page after deletion
            } else {
                console.error('Failed to delete booking request');
            }
        })
        .catch(error => {
            console.error('Error:', error);
        });
    }
}


